<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="link" type="sql" required="true" label="COM_AUTO79_FORM_LBL_CRON_LINK" description="COM_AUTO79_FORM_DESC_CRON_LINK" hint="COM_AUTO79_FORM_LBL_CRON_LINK"/>
    <field name="adcategories" type="sql" required="true" label="COM_AUTO79_FORM_LBL_CRON_ADCATEGORIES" description="COM_AUTO79_FORM_DESC_CRON_ADCATEGORIES" hint="COM_AUTO79_FORM_LBL_CRON_ADCATEGORIES"/>
    <field name="pageto" type="number" required="true" label="COM_AUTO79_FORM_LBL_CRON_PAGETO" description="COM_AUTO79_FORM_DESC_CRON_PAGETO" hint="COM_AUTO79_FORM_LBL_CRON_PAGETO"/>
    <field name="pagefrom" type="number" required="true" label="COM_AUTO79_FORM_LBL_CRON_PAGEFROM" description="COM_AUTO79_FORM_DESC_CRON_PAGEFROM" hint="COM_AUTO79_FORM_LBL_CRON_PAGEFROM"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
